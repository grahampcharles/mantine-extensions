{
  "compilerOptions": {
    "target": "es2019",
    // "lib": [
    //   "dom",
    //   "dom.iterable",
    //   "esnext"
    // ],
    "declaration": true,
    "declarationMap": true,
    "rootDir": ".",
    "composite": true,
    "outDir": "./dist",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true, "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "." /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],

    },
    
  },
  "include": [
      "**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ], 
  "references": []
}
